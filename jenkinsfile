#!/usr/bin/env groovy
@Library('grovvy_shared_library@main')_ 
node{
  
   
              stage('Source') {
                z= new git()
                z.checkOutFrom('webapp')
               		
              }
              stage('Build') {
                     bat "mvn package"
               }	
               stage ('Check logs') {
                       filterLogs ('WARNING', 1)
              }
   }
