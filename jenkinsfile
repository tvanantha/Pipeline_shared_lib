#!/usr/bin/env groovy
@Library('grovvy_shared_library@main')_ 
node{
  
    def rtMaven
    def buildInfo
  
              stage('Source') {
               		git 'https://github.com/tvanantha/webapp.git'
              }
              stage('Build') {
                     sh "mvn package"
               }	
               stage ('Check logs') {
                       filterLogs ('WARNING', 1)
              }
   }
