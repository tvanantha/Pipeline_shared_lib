#!/usr/bin/env groovy
@Library('grovvy_shared_library@main')_ 
node{
  
    def rtMaven
    def buildInfo
  
              stage('Source') {
               		git 'https://github.com/tvanantha/webapp.git'
              }
              stage('Maven build') {
              rtMaven.tool = "Maven-3.6.3"
              buildInfo = rtMaven.run pom: 'webapp/pom.xml', goals: 'clean install'
       	      }	
               stage ('Check logs') {
                       filterLogs ('WARNING', 1)
              }
   }
