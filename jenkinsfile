#!/usr/bin/env groovy
@Library('shared-library@master') _ 
node{
              stage('Source') {
               		git 'https://github.com/tvanantha/webapp.git'
                }

              stage('maven build'){
                  steps{
                      script{
		    	       sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
